{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "no-invalid-this": "off",
    "require-jsdoc": "off",
    "max-len": [
      "error",
      {
        "code": 100
      }
    ]
  }
}
